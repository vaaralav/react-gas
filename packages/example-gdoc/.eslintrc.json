{
  "extends": ["eslint:recommended", "prettier", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": ["googleappsscript", "prettier"],
  "env": {
    "googleappsscript/googleappsscript": true
  },
  "globals": {
    "global": false
  }
}
